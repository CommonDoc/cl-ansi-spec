;;;; setup-sections.tex
(in-package :ansi-spec.traverse)

;;; Where else can I put this?

(loop for mode in (list "secref" "chapref") do
  (let ((mode mode))
    (define-mode (mode)
                 :callbacks
                 ((()
                   ;; Nothing
                   )
                  ((node)
                   ;; This is the tag with the section reference
                   (output (format nil "<~a id=~S>" mode (plump:tag-name node))))))))

